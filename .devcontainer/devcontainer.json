{
  "name": "Snort",
  "service": "snort",
  "forwardPorts": [8080],
  "portsAttributes": {
    "8080": {
      "label": "Web",
      "onAutoForward": "notify"
    }
  },
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  }, 
  "dockerComposeFile": "codespaces.yml",
  "workspaceFolder": "/workspaces/snort"
}
